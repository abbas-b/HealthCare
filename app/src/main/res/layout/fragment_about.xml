        android:gravity="center_horizontal"
        android:textSize="45dp"
        android:text="ABOUT"
        android:textColor="@color/material_blue_grey_900"
        android:textStyle="bold"
        android:layout_gravity="center_horizontal|top" />

</FrameLayout>
